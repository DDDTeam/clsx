{
  "name": "ddd-clsx",
  "version": "1.0.3",
  "description": "CLSX made by DDD team",
  "main": "dist/ddd-clsx.mjs",
  "module": "dist/ddd-clsx.mjs",
  "style": "dist/ddd-clsx.css",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "type-check": "tsc --noEmit",
    "start": "vite",
    "build": "tsc && vite build && npm run build:types",
    "build:types": "tsc -p tsconfig.build.json"
  },
  "keywords": [
    "clsx",
    "ddd",
    "frontend",
    "typescript"
  ],
  "devDependencies": {
    "vite": "^7.2.7",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.1",
    "vite-plugin-pwa": "^1.1.0",
    "vite-plugin-static-copy": "^3.1.4",
    "vite-tsconfig-paths": "^5.1.4",
    "vite-plugin-svgr": "^4.5.0",
    "@types/node": "^24.7.2"
  }
}
